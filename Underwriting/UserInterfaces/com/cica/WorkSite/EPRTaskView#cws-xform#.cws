function Complete_click()
{    
	if(radio3.getValue()==0)
	{
	    // logic to get 14 days from current date
            var fortnightAway = new Date(Date.now() + 12096e5);
            var month = fortnightAway.getMonth() + 1;
            var day = fortnightAway.getDate();
            var year = fortnightAway.getFullYear();
            month = (month.toLocaleString().length==1?"0"+month:month);
            var dueDate = year+"-"+month+"-"+day;
            // end of logic 
            
            var request = cordys.cloneXMLDocument(TaskDetailsUpdate.XMLDocument);
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='TaskName']","Enrollment Platform Review");
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='MasterAppNumber']",masterAppNo.getValue());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='GroupNumber']",groupNumber.getValue());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='CompanyCode']",companyCodeOrLOB.getValue());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='TeamName']",taskPlatformName.getValue());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='Reason']",radio3.getValue());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='DueDate']",dueDate);
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='ReqNotes']",Comment);
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='CreatedBy']",Workflow.getAssigneeName());
            //cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='CreatedDate']",getCurrentDateandTime());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='CaseId']",Workflow.getCaseInstanceId());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='linkedTask']",Workflow.getActivityName());
            cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='TaskDetails']/*[local-name()='TaskId']",Workflow.getTaskInstanceId());	
			
            TaskDetails2Model.clear();
            TaskDetails2Model.setMethodRequest(request);
            TaskDetails2Model.reset();
            //TaskDetailsModel.synchronize();
            if(!TaskDetails2Model.soapFaultOccurred)
            {
                var PKeyId= cordys.getNodeText(TaskDetails2Model.getData(),".//*[local-name()='Id']");// to pass it to the task creation bpm
                // Logic to be written here to trigger task creation BPM
                var req = WKSCreateTaskModel.getMethodRequest();
                cordys.setNodeText(req,".//*[local-name()='ID']", PKeyId);
			 
                cordys.setNodeText(req,".//*[local-name()='TaskType']", "OPS");
                cordys.setNodeText(req,".//*[local-name()='UserDn']", Workflow.getAssigneeDN());
                WKSCreateTaskModel.setMethodRequest(req);
	        WKSCreateTaskModel.reset();
            }
        }
        updateTaskDetails();
        var request = CompleteOpsTaskModel.getMethodRequest();

	cordys.setNodeText(request,".//*[local-name()='TaskName']","EPS");
	cordys.setNodeText(request,".//*[local-name()='TaskCreatedUser']",Workflow.getSender());  
	cordys.setNodeText(request,".//*[local-name()='PolicyNumber']",masterAppNo.getValue());
	cordys.setNodeText(request,".//*[local-name()='TaskIntanceID']",Workflow.getTaskInstanceId());
	cordys.setNodeText(request,".//*[local-name()='CaseInstanceID']",Workflow.getCaseInstanceId());
	if(radio3.getValue()==0)
	    var mode = "Yes";
	else
	    var mode = "No";
	cordys.setNodeText(request, ".//*[local-name()='Mode']", mode);
	cordys.setNodeText(request, ".//*[local-name()='Action']", "");
	CompleteOpsTaskModel.setMethodRequest(request);
	CompleteOpsTaskModel.reset();

    window.parent.parent.parentWindow.closeCaseView();
}

