        var Notes_Type = (radio2.getValue()==0 ? "Private" : "Public" );
        var request = cordys.cloneXMLDocument(NotesUpdate.XMLDocument);
        //request = cordys.selectXMLNode(request,".//*[local-name()='new']/*[local-name()='Notes']")
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='PolicyNumber']",masterAppNo.getValue());
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='NotesType']",Notes_Type);
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='SiteId']","2");
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='Comment']",textarea1.getValue());
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='LoggedBy']",userForNotes);
        cordys.setNodeText(request,".//*[local-name()='new']/*[local-name()='Notes']/*[local-name()='Screen']","Enrollment Platform Setup");
        
        NotesModel.setMethodRequest(request);
