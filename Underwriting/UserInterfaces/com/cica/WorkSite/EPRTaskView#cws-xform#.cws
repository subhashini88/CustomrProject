document.getElementById("textarea1").addEventListener("keyup", function(e) {
	var max = 500;
        if (document.getElementById("textarea1").value.length == max) {
            //e.preventDefault();
        } else if (document.getElementById("textarea1").value.length > max) {
            // Maximum exceeded
            document.getElementById("textarea1").value = document.getElementById("textarea1").value.substring(0, max);
        }
});
function notesDetails()
{
    var request = Notes1Model.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='Policy_Number']",masterAppNo.getValue());
    cordys.setNodeText(request,".//*[local-name()='screen']","'Enrollment Platform Setup'");
    Notes1Model.setMethodRequest(request);
    Notes1Model.reset();
	
    //Notes history  
    notesHistory.setValue("");
    var text =""; 
    var len = cordys.selectXMLNodes(Notes1Model.getData(),".//*[local-name()='tuple']");
    var lengthTupl=len.length;
    var Tuple = cordys.selectXMLNodes(Notes1Model.getData(),".//*[local-name()='tuple']");
	
    for(var i=0 ; i<lengthTupl;i++)
	{
        var Comment = cordys.getNodeText(Tuple[i],".//*[local-name()='comment']");
        var Loggedby = cordys.getNodeText(Tuple[i],".//*[local-name()='logged_by']");
        var Loggedon = cordys.getNodeText(Tuple[i],".//*[local-name()='creation_time']");
        var NotesType= cordys.getNodeText(Tuple[i],".//*[local-name()='notes_type']");
        var Screen= cordys.getNodeText(Tuple[i],".//*[local-name()='screen']");
        var loggedDate = Loggedon.split("T")[0];
        var loggedTime = Loggedon.split("T")[1];
        loggedDate = loggedDate.split("-")[1]+"/"+loggedDate.split("-")[2]+"/"+loggedDate.split("-")[0];
        loggedTime = loggedTime.split(":")[0]+":"+loggedTime.split(":")[1];
        
        var text_row = Loggedby + " ; " + loggedDate +" "+loggedTime+" ; "+ Screen + " ; " + NotesType+"\n";
        var comments = Comment + "\n\n";
        text = text+text_row+comments;
    } 
    notesHistory.setValue(text) // set value for code snippet
    document.getElementById("notesHistoryDiv").innerHTML = text; 
}
function getTaskDetails()
{
    var request = TaskDetailsModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='TaskInstanceId']",taskID);

    TaskDetailsModel.setMethodRequest(request);
    TaskDetailsModel.reset();
    
    var SavedTaskDetails = TaskDetailsModel.getData();          
    
    taskPlatformName.setValue(cordys.getNodeText(SavedTaskDetails,".//*[local-name()='TeamName']"));
    taskDueDate.setValue(cordys.getNodeText(SavedTaskDetails,".//*[local-name()='DueDate']"));
    taskCreatedBy.setValue(cordys.getNodeText(SavedTaskDetails,".//*[local-name()='CreatedBy']"));
    radio3.setValue(cordys.getNodeText(SavedTaskDetails,".//*[local-name()='Reason']"));
    if(radio3.getValue() == "") 
        radio3.setValue(0);
    taskPKId =  cordys.getNodeText(SavedTaskDetails,".//*[local-name()='Id']");
    
var text="";
var Loggedby = cordys.getNodeText(SavedTaskDetails,".//*[local-name()='CreatedBy']");
Comment = cordys.getNodeText(SavedTaskDetails,".//*[local-name()='ReqNotes']");
var NotesType= "Public";
var ScreenName= cordys.getNodeText(SavedTaskDetails,".//*[local-name()='TaskName']");
var Screen = "Create Task - " + ScreenName;
var Loggedon = cordys.getNodeText(SavedTaskDetails,".//*[local-name()='CreatedDate']"); 
if(Loggedon !=undefined && Loggedon!="" && Loggedon !=null && Comment !=undefined && Comment!="" &&  Comment !=null){    
        var loggedDate = Loggedon.split("T")[0];
        var loggedTime = Loggedon.split("T")[1];
        loggedDate = loggedDate.split("-")[1]+"/"+loggedDate.split("-")[2]+"/"+loggedDate.split("-")[0];
        loggedTime = loggedTime.split(":")[0]+":"+loggedTime.split(":")[1]+":"+loggedTime.split(":")[2].split(".")[0];
        
        var text_row = Loggedby + " ; " + loggedDate +" "+loggedTime+" ; "+ Screen + " ; " + NotesType+"\n";
        var comments = Comment + "\n\n";
        text = text+text_row+comments; 
    requestorNotes.setValue(text);
    }
}
