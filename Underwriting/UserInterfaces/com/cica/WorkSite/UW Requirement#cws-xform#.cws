function checkthevalue(sentencearray, maintext) {


        if (maintext.length < maxLength ) {
            sentencearray.push(maintext);
            return sentencearray;
        }
        else {
            var substring = maintext.substring(0, maxLength );
            var remainingString = maintext.substring(maxLength+1, maintext.length);
            var n = substring.lastIndexOf(" ");
            if (n == substring.length) {
                sentencearray.push(substring);
                checkthevalue(sentencearray, remainingString)
            }
            else {
                var substring = maintext.substring(0, n);
                var remainingString = maintext.substring(n, maintext.length);
                sentencearray.push(substring);
                checkthevalue(sentencearray, remainingString)
            }
        }
    }


function getCurrentDate()
{
    //var currentDate = new Date(new Date().getTime() - (new Date().getTimezoneOffset() * 60 * 1000));
    var currentDate = new Date();
    var day = currentDate.getDate();
    if(day.toString().length==1)
    {
        day="0" + day;
    }
    var month = currentDate.getMonth() + 1;
    if(month.toString().length==1)
    {
        month="0" + month;
    }
    var year = currentDate.getFullYear();
    var date = year + '-' + month +  '-' + day;
 
    return date = date + " " +
                           currentDate.getHours() + ":" +
                           currentDate.getMinutes() + ":" +
                           currentDate.getSeconds() + ".000";
}

