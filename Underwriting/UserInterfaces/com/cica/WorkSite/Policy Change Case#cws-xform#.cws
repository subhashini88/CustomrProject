function ActivityLogDateTime(ActionDate)
{
var dateStr ="";
if(ActionDate != undefined){
var date = new Date(ActionDate);
	 var hours = date.getHours();
  var ampm = hours >= 12 ? 'PM' : 'AM';
dateStr =
  ("00" + (date.getMonth() + 1)).slice(-2) + "/" +
  ("00" + date.getDate()).slice(-2) + "/" +
  date.getFullYear() + " " +
  ("00" + date.getHours()).slice(-2) + ":" +
  ("00" + date.getMinutes()).slice(-2) + ":" +
  ("00" + date.getSeconds()).slice(-2)+" "+ampm;
  }
  return dateStr;
}
function ActivityLogDate(ActionDate)
{
