			<xformsscript>
				<![CDATA[var taskID="", dId="", activityTuple="", activeCases=0, PLANID="", currUser="", userDNForNotes="",found=false,tableFlag=true; 
var activitylogObj =[];
var taskObj = [];
function Form_InitDone(eventObject)
{
    choicebox1.hide();
    //textarea4.disable();
    submitNotesBtn.disable();
    hiddenGroup.hide();
    
 //getting case related details
    
    opId = "a0D7A000002NfrpUAC";
    caseHandlingType.setValue("Standard");
    
   // opId=masterAppID.getValue();
    //opCaseHandling=SpecialHandling.getValue();
    //caseHandlingType.setValue(opCaseHandling);
    
    getMasterAppObject(opId);
   KeyCompanyCode.setValue("CBS");
    keyGroupNumber.setValue("CWMA20993");
    KeyMasterApp.setValue("MA-23033");
    
   
    // task summary start
    GetActivitiesBySourceOperationModel.reset();
    activityTuple=cordys.selectXMLNodes(GetActivitiesBySourceOperationModel.getData(),".//*[local-name()='tuple']");
    
        for(var i =0;i<activityTuple.length;i++)
	{
	    
			var request = TaskDetailsModel.getMethodRequest();
			cordys.setNodeText(request,".//*[local-name()='TaskInstanceId']",cordys.getNodeText(activityTuple[i],".//*[local-name()='TaskId']",""));
			TaskDetailsModel.setMethodRequest(request);
			TaskDetailsModel.reset();
			var SavedTaskDetails = TaskDetailsModel.getData();          
			var dueDate= cordys.getNodeText(SavedTaskDetails,".//*[local-name()='DueDate']");
			var teamName = cordys.getNodeText(SavedTaskDetails,".//*[local-name()='TeamName']");
			var reason = cordys.getNodeText(SavedTaskDetails,".//*[local-name()='Reason']");
			var subactivity = "";
			teamName = teamName!=undefined?teamName:"";
                        reason = reason!=undefined?reason:"";
			if(reason!="")
				subactivity = teamName+"_"+reason;
			else
				subactivity = teamName;
			if(subactivity == undefined)
			    subactivity = "";
			if(dueDate == undefined)
			    dueDate = "";
			taskObj.push({
				taskStatus: cordys.getNodeText(activityTuple[i],".//*[local-name()='State']",""),
				taskType: cordys.getNodeText(activityTuple[i],".//*[local-name()='Activity']",""),
				taskSubType: subactivity,
				team: teamName,
				taskOwner: cordys.getNodeText(activityTuple[i],".//*[local-name()='Assignee']/@displayName",""),
				taskDueDate: dueDate,
				taskCreateDate: cordys.getNodeText(activityTuple[i],".//*[local-name()='StartDate']",""),
				taskClosedDate:cordys.getNodeText(activityTuple[i],".//*[local-name()='CompletionDate']","")
			});
		
	}
 
// end of task summary
        
    // activity log changes start here
    getClaimCaseActivityLogsModel.reset();
    var taskTuple=cordys.selectXMLNodes(getClaimCaseActivityLogsModel.getData(),".//*[local-name()='ActivityLog']");
       
        for(var i=0;i<taskTuple.length;i++)
        {
        var activity = cordys.getNodeText(taskTuple[i],".//*[local-name()='Activity']");
            var WorkItem = cordys.getNodeText(taskTuple[i],".//*[local-name()='WorkItem']");
            var user = cordys.getNodeText(taskTuple[i],".//*[local-name()='User']");
            var ActionDate = cordys.getNodeText(taskTuple[i],".//*[local-name()='ActionDate']");
	    var OldDesc = cordys.getNodeText(taskTuple[i],".//*[local-name()='OldDescription']");
	    var NewDesc = cordys.getNodeText(taskTuple[i],".//*[local-name()='NewDescription']");
	    
        if((activity=="Create Task") && (WorkItem=="" || WorkItem=="" )){
	var req = TaskDetailsModel.getMethodRequest();
	cordys.setNodeText(req,".//*[local-name()='TaskInstanceId']",cordys.getNodeText(taskTuple[i],".//*[local-name()='TaskId']"));
    TaskDetailsModel.setMethodRequest(req);
    TaskDetailsModel.reset();
    var Duedate = ActivityLogDate(cordys.getNodeText(TaskDetailsModel.getData(),".//*[local-name()='DueDate']"));
    NewDesc="Due Date: "+Duedate ;

	activitylogObj.push({
    	activityType: activity,
    	workItem: WorkItem,
    	User: user,
    	oldDesc: "",
    	newDesc: NewDesc,
    	actionDate: ActionDate
    });
    if(cordys.getNodeText(TaskDetailsModel.getData(),".//*[local-name()='ReqNotes']")!= undefined && cordys.getNodeText(TaskDetailsModel.getData(),".//*[local-name()='ReqNotes']")!= "")
    {
    activitylogObj.push({
    	activityType: "User Note",
    	workItem: "Create Task - "+WorkItem,
    	User: user,
    	oldDesc: "",
    	newDesc: "",
    	actionDate: ActionDate
    	});
    }
	}
	else
	{
       
				
	    activitylogObj.push({
                activityType: activity,
		workItem: WorkItem,
		User: user,
		oldDesc: OldDesc,
		newDesc: NewDesc,
		actionDate: ActionDate
	    });
	    }
	
        }
        
    //Activity type is user note
    var request = Notes1Model.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='Policy_Number']",masterAppNo.getValue());
    //cordys.setNodeText(request,".//*[local-name()='screen']","'Enrollment Case _ New','Enrollment Platform Setup'");
    Notes1Model.setMethodRequest(request);
    Notes1Model.reset();
 
    TaskNotes = Notes1Model.getData();
    var TaskNotesLength = cordys.selectXMLNodes(TaskNotes, ".//*[local-name()='tuple']").length;
    var TaskNotesTuple = cordys.selectXMLNodes(TaskNotes, ".//*[local-name()='tuple']");
    for(var tn=0;tn<TaskNotesLength;tn++){
    
        var activity = "User Note";
        var WorkItem = cordys.getNodeText(TaskNotesTuple[tn],".//*[local-name()='screen']");
        var user = cordys.getNodeText(TaskNotesTuple[tn],".//*[local-name()='logged_by']");
        var ActionDate = cordys.getNodeText(TaskNotesTuple[tn],".//*[local-name()='creation_time']");
	var OldDesc = "";
	var NewDesc = cordys.getNodeText(TaskNotesTuple[tn],".//*[local-name()='notes_type']");
	
	var dateStr = ActivityLogDateTime(ActionDate);
	
	
	activitylogObj.push({
    	activityType: activity,
    	workItem: WorkItem,
    	User: user,
    	oldDesc: OldDesc,
    	newDesc: NewDesc,
    	actionDate: dateStr
    });
    
    }
        //activity from review action summary table
    GetActivityModel.reset();
    
    var ActivityLength = cordys.selectXMLNodes(GetActivityModel.getData(), ".//*[local-name()='tuple']").length;
    var ActivityTuple = cordys.selectXMLNodes(GetActivityModel.getData(), ".//*[local-name()='tuple']");
    for(var tn=0;tn<ActivityLength ;tn++){
   
		  
        var activity = cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='ActionType']");
        var WorkItem = cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='WorkItem']");
        var user = cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='ActionBy']");
        var ActionDate = cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='ActionDate']");
	var OldDesc = (cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='OldDescription']")!= undefined)? cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='OldDescription']") : "";
	var NewDesc = (cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='Description']") != undefined)?cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='Description']") :"";
	
	var dateStr = ActivityLogDateTime(ActionDate);
	
	if(activity == "Data Update")
	{
	    OldDesc = "Task Follow-up Date: "+ActivityLogDate(cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='OldDescription']"));
	    NewDesc = "Task Follow-up Date: "+ActivityLogDate(cordys.getNodeText(ActivityTuple [tn],".//*[local-name()='Description']"));
	}
	else
	{
	}
	
	activitylogObj.push({
    	activityType: activity,
    	workItem: WorkItem,
    	User: user,
    	oldDesc: OldDesc,
    	newDesc: NewDesc,
    	actionDate: dateStr
    });
    
    }
    
    // sorting activity log
    activitylogObj.sort(function(a, b) {
        var dateA = new Date(a.actionDate), dateB = new Date(b.actionDate);
        return dateA - dateB;
    });
    // inserting activity log in table 
    for(var objar=activitylogObj.length-1;objar>=0;objar--){
        activityLogTable.create();
                var index = activityLogTable.getIndex();
                (activitylogObj[objar].activityType != undefined)?activityType[index].setValue(activitylogObj[objar].activityType):activityType[index].setValue("");
                (activitylogObj[objar].workItem!= undefined)?workItem[index].setValue(activitylogObj[objar].workItem):workItem[index].setValue("");
                (activitylogObj[objar].User!= undefined)?performedBy[index].setValue(activitylogObj[objar].User):performedBy[index].setValue("");
                (activitylogObj[objar].oldDesc!= undefined)?oldDesc[index].setValue(activitylogObj[objar].oldDesc):oldDesc[index].setValue("");
                (activitylogObj[objar].newDesc!= undefined)?newDesc[index].setValue(activitylogObj[objar].newDesc):newDesc[index].setValue("");
                (activitylogObj[objar].actionDate!= undefined)?actionDate[index].setValue(activitylogObj[objar].actionDate):actionDate[index].setValue("");
                
               /* workItem[index].setValue(activitylogObj[objar].workItem);
                performedBy[index].setValue(activitylogObj[objar].User);
                oldDesc[index].setValue(activitylogObj[objar].oldDesc);
                newDesc[index].setValue(activitylogObj[objar].newDesc);
                actionDate[index].setValue(activitylogObj[objar].actionDate);*/
                
                activityType[index].disable();
                workItem[index].disable();
                performedBy[index].disable();
                oldDesc[index].disable();
                newDesc[index].disable();
                actionDate[index].disable();
    }
    document.getElementById("activityLogTable").deleteRow(1)
        var logLen = activityLogTable.getIndex();
        if(logLen == -1 )
        logLen =0;
        groupbox2.setLabel("Activity Log ("+logLen+")")
        
        
    // task summary table
    for(var i=0;i<taskObj.length;i++)
    {
        taskTable.create();
        var index = taskTable.getIndex();
        taskStatus[index].setValue(taskObj[i].taskStatus);
	taskType[index].setValue(taskObj[i].taskType);
	taskSubType[index].setValue(taskObj[i].taskSubType);
	//team[index].setValue(taskObj[i].team);
	tbl_taskOwner[index].setValue(taskObj[i].taskOwner);
	tbl_dueDate[index].setValue(taskObj[i].taskDueDate);
	tbl_createdDate[index].setValue(taskObj[i].taskCreateDate);
	tbl_closedDate[index].setValue(taskObj[i].taskClosedDate);

    }

    
    for(var i =0,j=0;i<document.getElementById("taskTable").children[1].children.length;i++)
    {
	j=i+1;
	if(document.getElementById("taskTable").children[1].children[i].style[0] == "display")
		document.getElementById("taskTable").deleteRow(j);
	else
		continue;
    }
    groupbox3.setLabel("Task Summary ("+taskObj.length+")");
    
    //checking if assignee is same as current user- View/Edit Mode
    taskID = Workflow.getTaskInstanceId();
     
    var request = TaskModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='TaskId']",taskID);
    cordys.setNodeText(request,".//*[local-name()='Target']","");
