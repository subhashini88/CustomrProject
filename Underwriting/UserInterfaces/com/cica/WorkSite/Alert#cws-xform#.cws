function notesDetails()
{
    var request = Notes2Model.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='TaskId']",Workflow.getTaskInstanceId());
    Notes2Model.setMethodRequest(request);
    Notes2Model.reset();
	
    //Notes history  
   // notesHistory.setValue("");
    var text =""; 
    var len = cordys.selectXMLNodes(Notes2Model.getData(),".//*[local-name()='tuple']");
    var lengthTupl=len.length;
    var Tuple = cordys.selectXMLNodes(Notes2Model.getData(),".//*[local-name()='tuple']");
	
    for(var i=0 ; i<lengthTupl;i++)
	{
        var Comment = cordys.getNodeText(Tuple[i],".//*[local-name()='Comment']");
        var Loggedby = cordys.getNodeText(Tuple[i],".//*[local-name()='LoggedBy']");
        var Loggedon = cordys.getNodeText(Tuple[i],".//*[local-name()='CreationTime']");
        var NotesType= cordys.getNodeText(Tuple[i],".//*[local-name()='NotesType']");
        var Screen= cordys.getNodeText(Tuple[i],".//*[local-name()='Screen']");
        var loggedDate = Loggedon.split("T")[0];
        var loggedTime = Loggedon.split("T")[1];
        loggedDate = loggedDate.split("-")[1]+"/"+loggedDate.split("-")[2]+"/"+loggedDate.split("-")[0];
        //loggedTime = loggedTime.split(":")[0]+":"+loggedTime.split(":")[1];
        loggedTime = loggedTime.split(":")[0]+":"+loggedTime.split(":")[1]+":"+loggedTime.split(":")[2].split(".")[0];
        
        text_row = Loggedby + " ; " + loggedDate +" "+loggedTime+" ; "+ Screen + " ; " + NotesType+"\n";
        //textrow = Loggedby + " ; " + loggedDate +" "+loggedTime+" ; Enrollment Platform Review ; " +NotesType+"\n";
        var comments = Comment + "\n\n";
        //text = text+text_row+comments;
        text = text+text_row.italics()+comments;
    } 
    //notesHistory.setValue(text) // set value for code snippet
    document.getElementById("notesHistoryDiv").innerHTML = text; 
}
