function Save_click()
{
    if(new Date(taskFollowUpDt.getValue()) > new Date() || taskFollowUpDt.getValue() == "" )
    {	
        WorkList= cordys.getNodeText(Workflow.getTaskOutputData(),".//*[local-name()='Targets']");
        WorkList= WorkList.split(",")[0];
        WorkList= WorkList.split("=")[1];
        if(WorkList=="Worksite Enrollment")
        {
            TaskTransferToUWPendingWorklistModel.reset();
            
        }
        else
        {
            // to update the followup date in pending worklist
            updateFollowUpDetails();
        } 
        updateTaskDetails();
        window.parent.parent.parentWindow.closeCaseView();
    }
	
    else
    {
        application.notify("Task Follow Up date can not be less than today's date","ERROR");
        return;
    } 
   
}
