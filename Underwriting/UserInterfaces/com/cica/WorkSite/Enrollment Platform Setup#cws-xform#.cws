function Form_InitDone(eventObject)
{
    //hiding fields
    worklistTaskIdentifiersGroupbox.hide();
    
    //setting style property
                        document.getElementById("group9").children[0].children[0].children[0].setAttribute("style","color:black!important");
//document.getElementById("requestornotesDiv").setAttribute("style","color:black!important");
document.getElementById("group36").children[0].children[0].setAttribute("style","color:black!important");
document.getElementById("group9").children[0].children[0].children[1].setAttribute("style","margin-top:-1%");
document.getElementById("taskFollowUpDt").setAttribute("style","height: 30px !important;color: #555 !important;background-color: #fff !important;background-image: none !important;border: 1px solid #ccc !important;border-radius: 4px !important;");

    document.getElementById("textarea1").setAttribute("style","color: black !important;font-size: 15px !important;left: 0px;top: 0px;right: 0px;bottom: 0px;width: 100%;cursor: auto;height: 100%;overflow-x: hidden;overflow-y: auto;resize: none;");
    
    //disabling fields
    accountName.disable();
    //requestorNotes.disable();
    //textarea1.disable();
    submitNotesBtn.disable();
    
    //getting case related details
    opId=masterAppID.getValue();
    opCaseHandling=SpecialHandling.getValue();
    caseHandlingType.setValue(opCaseHandling);
    getMasterAppObject(opId);
    
    //checking if assignee is same as current user- View/Edit Mode
    taskID = Workflow.getTaskInstanceId();
     
    var request = TaskModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='TaskId']",taskID);
    cordys.setNodeText(request,".//*[local-name()='Target']","");
    TaskModel.setMethodRequest(request);
    TaskModel.reset();
    assignee=cordys.getNodeText(TaskModel.getData(),".//*[local-name()='Assignee']");
   
    var split2= assignee.split(",");
    var assigneeName1= split2[0].split("=");
    var assigneeName=assigneeName1[1];

    var userdn=system.getUser().authUserDN;
    var split1= userdn.split(",");
    var currUser1= split1[0].split("=");
    var currUser=currUser1[1];
    userDNForNotes= userdn.replace(userdn.substring(userdn.indexOf(',')),assignee.substring(assignee.indexOf(',')));	
    if(assigneeName==currUser)
    {
        text7.show();
        text1.hide();
    }
    else
    {
        text1.show();
        text7.hide();
        taskFollowUpDt.disable();
    }
    
    
    //setting default peer view
    radio3.setValue(0);
    //group20.hide();
    
    //default notes
     radio2.setValue(1);
     radio2.disable();
    
    //for task details  
    getTaskDetails();
    
    //for displaying notes history
    notesDetails();
    
    //for pending worklist 
    WorkList= cordys.getNodeText(Workflow.getTaskOutputData(),".//*[local-name()='Targets']");
    WorkList= WorkList.split(",")[0];
    WorkList= WorkList.split("=")[1];
    
    WIMSOpsFollowUpsModel.reset();


}
