			<xformsscript>
				<![CDATA[var selectedRequest = "";

function dialogReturnValue()
    {
    var data = new Object();
      
		data.Type = selectedRequest;
		
                return data;
    }

function manualTask_Click(eventObject)
{
  selectedRequest  = 'Manual Task';
     application.container.close();
}
function Inter_CloseHandler(){
}

function cancelButton_Click(eventObject)
{
    selectedRequest  = 'Cancel';
    application.container.close();
}
function RequestTask_Click(eventObject)
{
    selectedRequest  = 'Create Task';
     application.container.close();
}
function Form_InitDone(eventObject)
{
    application.setDialogSize("461px","95px");
    if(application.event.data != undefined && application.event.data != '' && application.event.data != null){
   // if(application.event.data.screen=="UW Service Case")
   // manualTask.hide();
    //application.setDialogSize("461px","95px");
    }
}
]]>
