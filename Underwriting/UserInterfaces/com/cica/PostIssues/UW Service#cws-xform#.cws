function setUWManualNotes(){
//document.getElementById("notesHistory").style.cssText = 'display:none;'
	var Notes = cordys.getNodeText(GetUWManualTaskDetailModel.getData(),".//*[local-name()='Notes']");
	var parser = new DOMParser();
	var allNotesxml = parser.parseFromString(Notes, "text/xml");
	var allNotes = cordys.selectXMLNodes(allNotesxml,".//*[local-name()='Notes']");
	for(var i=0;i<allNotes.length;i++){
		var finalText = "";
		
		var name = cordys.getNodeText(allNotes[i],".//*[local-name()='logged_by']");
		var noteDate = cordys.getNodeText(allNotes[i],".//*[local-name()='creation_time']");
		var noteData = cordys.getNodeText(allNotes[i],".//*[local-name()='notes']");
		noteDate = noteDate.split('T')[0].split('-')[1]+"/"+noteDate.split('T')[0].split('-')[2]+"/"+noteDate.split('T')[0].split('-')[0]+" "+noteDate.split('T')[1];
		var servType = cordys.getNodeText(GetUWManualTaskDetailModel.getData(), ".//*[local-name()='ServiceType']");
		var taskType = cordys.getNodeText(allNotes[i],".//*[local-name()='action_type']");
		
		var NotesSub = name+";"+noteDate+";"+"UW "+taskType+"-"+servType+";";
		NotesSub = NotesSub.italics()+noteData;
		//NotesSub = NotesSub.fontcolor("gray");
		
		var notesscreendata = document.getElementById("notesHistory").innerHTML;
		
		if(notesscreendata=="")
		finalText = notesscreendata+"<br>"+NotesSub;
		else finalText = notesscreendata+"<br><br>"+NotesSub;
		
		document.getElementById("notesHistory").innerHTML = finalText;
	}
}
