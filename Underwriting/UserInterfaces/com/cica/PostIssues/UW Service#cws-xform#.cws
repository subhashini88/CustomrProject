function twoDigits(d) {   //For Tasktimer - Sarada Nayak
		if (0 <= d && d < 10)
			return "0" + d.toString();
		if (-10 < d && d < 0)
			return "-0" + (-1 * d).toString();
		return d.toString();
	}
