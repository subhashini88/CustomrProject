function updateTaskDetails(taskInstanceId,followUpAction,FollowupDate,decision)
{
    var reqMethod=cordys.cloneXMLDocument(updateTaskDetailsModel.XMLDocument);
    cordys.setNodeText(reqMethod,".//*[local-name()='old']/.//*[local-name()='PITaskId']",taskID);
    cordys.setNodeText(reqMethod,".//*[local-name()='new']/.//*[local-name()='PITaskId']",taskID);
    cordys.setNodeText(reqMethod,".//*[local-name()='new']/.//*[local-name()='TaskInstanceId']",taskInstanceId);
    cordys.setNodeText(reqMethod,".//*[local-name()='new']/.//*[local-name()='FollowUpAction']",followUpAction);
    cordys.setNodeText(reqMethod,".//*[local-name()='new']/.//*[local-name()='FollowupDate']",taskFollowUpDt);
    cordys.setNodeText(reqMethod,".//*[local-name()='new']/.//*[local-name()='Decision']",decision);
    DummyModel1.clear();
    DummyModel1.setMethodRequest(reqMethod);
    DummyModel1.reset()
    if(DummyModel1.soapFaultOccurred)
    {
        return false;
    }
    return true;
} 
