function btnComplete_Click(eventObject)
{

 if ( !WebForm.validate(application.container.applicationId))
 {
 application.showError('Validation Error');
  }
  else
  {

if (Decision.getValue() != "")

{
     var curdate = getCurrentDateDecision();
        DecisionDate.setValue(curdate )
     GetPostIssueDetailsUsingCaseIdModel.synchronize();
     debugger;
     if(Source.getValue()=="ClaimCase")
     {
     ClaimsRescissionUpdate();
     }
    // ClaimsRescissionUpdate();
     WorksheetUpdate();
     Workflow.showNoFollowupsPage();
     Workflow.completeTask();
 }

 else
    {
            application.showError ('Please select a Valid Decision');
    }  
    
    
    }   

}
function btnSave_Click(eventObject)
{       
     if ( !WebForm.validate(application.container.applicationId))
 {
return;
  }
  else
  {
    WorksheetUpdate();
    GetPostIssueDetailsUsingCaseIdModel.synchronize();
    application.inform("Details have been saved successfully");
    
    }
}
function Form_InitDone(eventObject)
{
    TaskStatus.hide();
Source.hide();
claimsComments.hide() //added as business ask not to show this for PIRT Jet issue 8 Feb 2023
worklistTaskIdentifiersGroupbox.hide();
claimsComments.disable();
ApplicationNumber.disable();
        DecisionDate.hide();

     CaseInstanceId = Workflow.getCaseInstanceId();
    GetPostIssueDetails();
    var commRequest= tblPostIssueworksheetCommentsModel.getMethodRequest();
    cordys.setNodeText(commRequest,".//*[local-name()='CaseID']",CaseInstanceId );
    tblPostIssueworksheetCommentsModel.setMethodRequest(commRequest);
    tblPostIssueworksheetCommentsModel.reset();
	//var commResp=tblPostIssueworksheetCommentsModel.getData();
	var commResp=cordys.selectXMLNodes(tblPostIssueworksheetCommentsModel.getData(),".//*[local-name()='tuple']");
	var ClaimsComment=cordys.getNodeText(commResp[0],".//*[local-name()='comment']")
	claimsComments.setValue(ClaimsComment.substring(ClaimsComment.indexOf('Comment: ') + 9)); 
    if  (Decision.getValue() !="")
    {}
    else
    {
        Decision.setValue("");  
    }
    
    

   
}

function GetPostIssueDetails()
{
    var request = GetPostIssueDetailsUsingCaseIdModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='CaseId']",CaseInstanceId);
    GetPostIssueDetailsUsingCaseIdModel.setMethodRequest(request);
    GetPostIssueDetailsUsingCaseIdModel.reset();
    GetPostIssueDetailsUsingCaseIdModel.refreshAllViews();
}

function ClaimsRescissionUpdate()
{
    var request = spInsertRescissiondataInClaimsModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='CaseInstanceId']",CaseInstanceId);
    cordys.setNodeText(request,".//*[local-name()='notes']",TaskComments.getValue()); 
    cordys.setNodeText(request,".//*[local-name()='RETURN_VALUE']","");                                                                                                                               
    spInsertRescissiondataInClaimsModel.setMethodRequest(request);
    spInsertRescissiondataInClaimsModel.reset();
    spInsertRescissiondataInClaimsModel.refreshAllViews();
}
function WorksheetUpdate()
{
	var todayDate = getCurrentDate();
    var loggedinuser = cordys.getNodeText(CordysRoot.getAssertions(), ".//*[local-name()='NameIdentifier']");
	var interactiontype = "Decision Submitted";
	var ipcomment = "Decision: " +Decision.getValue()+"; Task Comments: "+TaskComments.getValue();
	var interactiondetail =  todayDate+" CST/ - "+loggedinuser ;
	var worksheet_namespaces = {tns:"http://schemas.cordys.com/uw/dataservices/1.0"};
    var request = cordys.cloneXMLDocument(worksheetupdate.XMLDocument)  
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:policy_number",ApplicationNumber.getValue(),worksheet_namespaces);
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:interaction_type",interactiontype,worksheet_namespaces);
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:interaction_detail",interactiondetail ,worksheet_namespaces);
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:comment",ipcomment ,worksheet_namespaces);
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:case_id",CaseInstanceId,worksheet_namespaces);
    cordys.setNodeText(request,".//*[local-name()='new']/tns:tblPostIssueDetails_worksheet/tns:task_id","" ,worksheet_namespaces);
    UpdateTblPostIssueDetails_worksheetModel.setMethodRequest(request);
    UpdateTblPostIssueDetails_worksheetModel.reset();
}

function getCurrentDate()
{
    var currentDate = new Date();
    var day = currentDate.getDate();
    if(day.toString().length==1)
    {
        day="0" + day;
    }
    var month = currentDate.getMonth() + 1;
    if(month.toString().length==1)
    {
        month="0" + month;
    }
    var year = currentDate.getFullYear();
    var date =  year+  '-' + month + '-' + day;
    return date = date + "T" +
                           currentDate.getHours() + ":" +
                           currentDate.getMinutes() + ":" +
                           currentDate.getSeconds() + ".0";
}
function getCurrentDateDecision()
{
    var currentDate = new Date();
    var day = currentDate.getDate();
    if(day.toString().length==1)
    {
        day="0" + day;
    }
    var month = currentDate.getMonth() + 1;
    if(month.toString().length==1)
    {
        month="0" + month;
    }
    var year = currentDate.getFullYear();
    var date =  year+  '-' + month + '-' + day;
    return date = date + "T00:00:00.0";
}
function Form_Init(eventObject)
{
    if(window.parent.taskModel!=undefined)
        Init_TM();
}
function Form_BeforeClose(eventObject)
{
    if(window.parent.taskModel!=undefined)
        unloadFunction_TM();
}
