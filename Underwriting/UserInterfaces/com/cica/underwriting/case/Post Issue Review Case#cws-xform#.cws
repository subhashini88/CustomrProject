				<![CDATA[var CaseInstanceId,input_policy_number,CaseState;
//Init_TM();
function btn_lognote_Click(eventObject)
    {
            var task_owner = Workflow.getAssigneeName();
            var inttype = 'Note logged from Post Issue Review Case';

            var data = new Object();
            data.policynumber = input_policy_number ;
            data.taskowner = task_owner;
            data.CaseId = CaseInstanceId;
            
            data.InteractionType= inttype ;
        
        application.select(Postissue_lognote.XMLDocument.documentElement, data, null);
    }
    
function btn_Worksheet_Click(eventObject)
{
      var data = new Object();
      data.policynumber = input_policy_number ;
      data.CaseId = CaseInstanceId;
      application.select(Postissue_UnderwriterWorksheet.XMLDocument.documentElement, data, null);
}

function getCaseInstance()
{

    
    var request = GetCaseInstanceModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='caseinstanceid']",CaseInstanceId);
    GetCaseInstanceModel.setMethodRequest(request);
    GetCaseInstanceModel.reset();
    GetCaseInstanceModel.refreshAllViews();
    var a = GetCaseInstanceModel.getData();
    CaseState= cordys.getNodeText(a,".//*[local-name()='CURRENTSTATE']");
}
function btnSave_Click(eventObject)
{
 if ( !WebForm.validate(application.container.applicationId))
 {
return;
  }
  else
  {
  
    GetPostIssueDetailsUsingCaseIdModel.synchronize();
    application.inform("Details have been saved successfully");
    
    }
    
}

function btnExit_Click(eventObject)
{
    Workflow.taskView.close(); 
    }



function Form_InitDone(eventObject)
{
 CaseInstanceId = Workflow.getCaseInstanceId();
 worklistTaskIdentifiersGroupbox.hide();
 ApplicationNumber.disable();
IssueStateProvince.disable();
CustomerID.disable();
ReceivedDate.disable();
ProductShortDescription.disable();
FullProductDescription.disable();

    GetPostIssueDetails();
    TaskOwner.setValue ( Workflow.getAssigneeName())
    
    input_policy_number = ApplicationNumber.getValue();
    getCaseInstance();
    
    if (CaseState != "Closed") {
    
    opStatus.hide();
        if (Workflow.isActionPossible("CLAIM")) {
        Workflow.claimTask();
        

    }


}
else 
{
    btnSave.hide();
    btnExit.hide();
    if (Workflow.isActionPossible("REVOKE")){
    Workflow.revokeTask();}
}

}

function GetPostIssueDetails()
{
    var request = GetPostIssueDetailsUsingCaseIdModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='CaseId']",CaseInstanceId);
    GetPostIssueDetailsUsingCaseIdModel.setMethodRequest(request);
    GetPostIssueDetailsUsingCaseIdModel.reset();
    GetPostIssueDetailsUsingCaseIdModel.refreshAllViews();
    }
    
function SearchImage()
{

    input_policy_number = ApplicationNumber.getValue();
    var b = GetXMLObjectOperationModel.getData(); 
    var cmlink = cordys.getNodeText(b, ".//*[local-name()='cmLink']", "");
    var link =  cmlink + input_policy_number + " \"}]&autoRunSearch=true";
    var windowName = "CM - " + input_policy_number;
    window.open(link, windowName, width = 600, height = 300);
}    
function GetXMLObjectOperationModel_OnRequest(eventObject)
{
    cordys.setNodeText(eventObject.request, ".//*[local-name()='key']", '/com/cica/uw/UnderwritingConfig.xml');

}
function coverageHistory() {
debugger;
    var ServiceCenterInput;
    var input_company_code ="";
    input_policy_number = ApplicationNumber.getValue();    
    var request = GetUnderwritingRecordForPolicyNumberModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='PolicyNumber']",input_policy_number );
    GetUnderwritingRecordForPolicyNumberModel.setMethodRequest(request);
    GetUnderwritingRecordForPolicyNumberModel.reset();
    var a = GetUnderwritingRecordForPolicyNumberModel.getData();
    input_company_code = cordys.getNodeText(a,".//*[local-name()='company_code']");    

    if ((input_company_code == "NL") || (input_company_code == "NS")) {
        ServiceCenterInput = "03";
    } else if ((input_company_code == "EL") || (input_company_code == "ES")) {
        ServiceCenterInput = "01";
    } else if ((input_company_code == "WL") || (input_company_code == "WS")) {
        ServiceCenterInput = "02";
    } else {

        application.showError('Error in Fetching Company code - Contact IT');
        return;
    }
    var customerId =  CustomerID.getValue();
    var b = GetXMLObjectOperationModel.getData();
    var coverageinfolink = cordys.getNodeText(b, ".//*[local-name()='callcenterInfoLink']", "");
    var link = coverageinfolink +  "?serviceCenter=" + ServiceCenterInput + "&policyHolderId=" + customerId +"&reqId=0110&mask=true";
    var windowName = "CoverageInfo - " + input_policy_number;
    window.open(link, windowName, width = 600, height = 300);



}
function claimHistory() {
    var ServiceCenterInput;
        var input_company_code ="" ;
    input_policy_number = ApplicationNumber.getValue();    
    var request = GetUnderwritingRecordForPolicyNumberModel.getMethodRequest();
    cordys.setNodeText(request,".//*[local-name()='PolicyNumber']",input_policy_number );
    GetUnderwritingRecordForPolicyNumberModel.setMethodRequest(request);
    GetUnderwritingRecordForPolicyNumberModel.reset();
    var a = GetUnderwritingRecordForPolicyNumberModel.getData();
    input_company_code = cordys.getNodeText(a,".//*[local-name()='company_code']");
            

    if ((input_company_code == "NL") || (input_company_code == "NS")) {
        ServiceCenterInput = "03";
    } else if ((input_company_code == "EL") || (input_company_code == "ES")) {
        ServiceCenterInput = "01";
    } else if ((input_company_code == "WL") || (input_company_code == "WS")) {
        ServiceCenterInput = "02";
    } else {

        application.showError('Error in Fetching Company code - Contact IT');
        return;
    }
    var customerId =  CustomerID.getValue();
    var b = GetXMLObjectOperationModel.getData();
    var claimsinfolink = cordys.getNodeText(b, ".//*[local-name()='callcenterInfoLink']", "");
    var link = claimsinfolink + "?reqId=0222&serviceCenter=" + ServiceCenterInput + "&personId=" + customerId + "&groupName=";
    var windowName = "ClaimsInfo - " + input_policy_number;
    window.open(link, windowName, width = 600, height = 300);
}

function Form_Init(eventObject)
{
     if (Workflow.isActionPossible("CLAIM")) {
        Workflow.claimTask();
        

    }
    if(window.parent.taskModel!=undefined)
        Init_TM();
}
function Form_BeforeClose(eventObject)
{
    if(window.parent.taskModel!=undefined)
        unloadFunction_TM();
}
